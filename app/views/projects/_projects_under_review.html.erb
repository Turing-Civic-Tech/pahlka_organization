<h2> Under Review </h2>
<% @projects_under_review.each do |project| %>
<div>
  <div class="picolumn">
    <div class="picard">
      <%= image_tag project.image_path, style:"width:100%" %>
      <div class="picontainer">
        <h3 class='pititle'> <%= project.title %> </h3>
        <p> <%= project.description %>
        </p>
        <p> <%= link_to 'GitHub', project.github_link %>
            <%= link_to 'Heroku', project.heroku_link %>
        </p>
        <%= button_to "Edit", edit_project_path(project), class: 'projectbutton', method: :get  %>
        <%= button_to "Delete", project_path(project), method: :delete, class: 'projectbutton', :data => {:confirm => "Really delete #{project.title}?"} %>
        <%= button_to "Accept", accept_project_path(project), method: :post, class: 'projectbutton', :data => {:confirm => "Accept #{project.title}?"} %>
      </div>
    </div>
  </div>
</div>
<% end %>
