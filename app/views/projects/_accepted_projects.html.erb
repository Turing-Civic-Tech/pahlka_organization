<h2> Accepted </h2>
<% @accepted_projects.each do |project| %>
<div>
  <div class="picolumn">
    <div class="picard">
      <%= image_tag project.image_path, style:"width:100%" %>
      <div class="picontainer">
        <h3 class='pititle'> <%= project.title %> </h3>
        <p> <%= project.description %>
        </p>
        <p> <%= link_to 'GitHub', project.github_link %>
            <%= link_to 'Heroku', project.heroku_link %>
        </p>
      </div>
    </div>
  </div>
</div>
<% end %>
